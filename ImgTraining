package com.example.uilogin.obj;

public class ImgTraining {
    public double entropi;
    public double mean;
    public double energi;
    public double homogenitas;
    public double contrast;
    public int label;
    public String name;
    public String key;

    public ImgTraining(){

    }

    public ImgTraining(String name,double entropi, double mean,double energi, double homogenitas, double contrast,int label){

        this.name=name;
        this.entropi = entropi;
        this.mean = mean;
        this.energi = energi;
        this.homogenitas = homogenitas;
        this.contrast = contrast;
        this.label = label;
    }

    public void setKey(String key){
        this.key=key;
    }
}
